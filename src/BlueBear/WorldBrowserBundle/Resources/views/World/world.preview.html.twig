{# @var result \Sidus\EAV\World #}
{% set icePercentage = 100/result.climate.temperatureAverage %}
{% set water_percentage = result.climate.waterPercentage %}
{% set seed = result.worldCode %}
<a href="{{ admin_path('map', 'worldMap', {id: result.id}) }}"
   title="{{ ('admin.action.edit.label') | trans }}">
    <img src="http://worldgen.bin.sh/worldgen.cgi?palette=Atlas&iter=20&cmd=Create&pct_ice={{ icePercentage }}&height=100&seed={{ seed }}&projection=Animated&pct_water={{ water_percentage }}&motif=SciFi">
</a>
